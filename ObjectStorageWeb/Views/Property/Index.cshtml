@model List<Dictionary<string, string>>
@{
    ViewData["Title"] = "Edit";
}
<h3>New property</h3>
<form method="post">
    <input name="DisplayName" placeholder="Insert Property Name" required="required"/>
    <select name="Type" required="required">

        <option value="string">text</option>
        <option value="int">integer</option>
        <option value="float">decimal number</option>
        @foreach (var type in Model)
        {
            <option value="@type["type"]">@type["name"]</option>
        }
    </select>
    <input name="Constraint.MinValue" placeholder="Insert Min Value" type="number"/>
    <input name="Constraint.MaxValue" placeholder="Insert Max Value" type="number"/>
    <input type="submit"/>
</form>